{
  "rules": {
    "quotemark": {
      "options": "single",
      "severity": "warn"
    },
    "ordered-imports": {
      "severity": "warn",
      "options": [{
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }]
    },
    "no-unused-variable": false,
    "trailing-comma": true,
    "semicolon": true,
    "new-parens": true
  }
}